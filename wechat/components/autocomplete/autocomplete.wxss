/* components/autocomplete/autocomplete.wxss */

.autocomplete {
  position: relative;
}

.input {
  height: 60rpx;
  line-height: 60rpx;
}

.lists {
  position: absolute;
  top: 60rpx;
  left: 0;
  z-index: 5;
  width: 100%;
  height: 500rpx;
  box-sizing: border-box;
  padding: 20rpx;
  border-radius: 10rpx;
  box-shadow: 0 0 10rpx 1rpx rgba(0, 0, 0, 0.3);
  border: 1px solid #ddd;
  overflow: hidden auto;
  background: #fff;
}

.list {
  height: 60rpx;
  line-height: 60rpx;
  border-bottom: 1px solid #ddd;
}
